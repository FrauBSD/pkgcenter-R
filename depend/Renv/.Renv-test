cat("# Begin .Renv\n")
cat("# Library paths are:\n")
for(d in .libPaths()) {
	cat(sprintf("#   %s\n", d))
}
args = commandArgs(trailingOnly=TRUE)
argc = length(args)
cat(sprintf("# There are %d args\n", argc))
if (argc > 0) {
	for(i in 1:argc) {
		cat(sprintf("#   Arg%i is %s\n", i, args[i]))
	}
}
cat("# End .Renv\n")
